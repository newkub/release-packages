{
  "name": "release-packages",
  "version": "0.0.0",
  "description": "A release script using clack prompts",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "release": "./dist/index.mjs"
  },
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --watch --exports --dts",
    "dev": "bun --watch src/index.ts",
    "release": "bun run src/index.ts",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "release",
    "cli",
    "automation",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.7.0",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3",
    "zod-to-json-schema": "^3.24.6"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "effect": "^3.18.4"
  }
}